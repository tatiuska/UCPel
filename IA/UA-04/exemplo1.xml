<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<soap:Envelope xmlns:soap="http://www.w3.org/05/soap-envelope/" soap:encodingStyle="http://www.w3.org/2003/05/soap-encoding">
    <!-- Elemento raiz do SOAP que define que essa é uma mensagem SOAP -->
    <soap:Header>
        <!-- Informações específicas como autenticação (opcional) -->
    </soap:Header>
    <soap:body>
        <!-- Elemento que contém o corpo da mensagem -->
        <soap:Fault>
            <!-- Elemento que contém os erros que podem ocorrer -->
        </soap:Fault>
    </soap:body>
</soap:Envelope>